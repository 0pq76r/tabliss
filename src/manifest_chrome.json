{
  "manifest_version": 2,
  "name": "Tabliss - A Beautiful New Tab",
  "short_name": "Tabliss",
  "version": "1.15.0",
  "description": "A beautiful New Tab page with many customisable backgrounds and widgets that does not require any permissions.",
  "background": {
     "persistent": false,
     "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Tabliss"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  }
}
