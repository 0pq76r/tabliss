{
  "manifest_version": 2,
  "name": "Tabliss",
  "short_name": "Tabliss",
  "version": "1.6.1",
  "description": "A beautiful new tab page with many customisable backgrounds and widgets. See stunning new landscapes and photos in every new tab.",
  "applications": {
    "gecko": {
      "id": "extension@tabliss.io",
      "strict_min_version": "54.0"
    }
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  }
}
